<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="s_switch" parent="Widget.AppCompat.CompoundButton.Switch">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/fs_text_16</item>
        <item name="android:textFontWeight">400</item>
        <item name="trackTint">@color/selector_switch_track</item>
        <item name="thumbTint">@color/selector_switch_thumb</item>
    </style>

    <style name="s_text_view_setting">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/fs_text_16</item>
        <item name="android:textFontWeight">400</item>
        <item name="drawableTint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>

    <style name="s_text_view_history">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="s_button_clear_history">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:text">@string/c_history_clear</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="cornerRadius">54dp</item>
    </style>

    <style name="placeHolderTextStyle">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/fs_text_19</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="placeHolderButton" parent="Widget.MaterialComponents.Button">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">@integer/fw_500</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/fs_text_14</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="cornerRadius">@dimen/i_p_54</item>
        <item name="android:shadowRadius">0</item>
    </style>

    <style name="s_button_error_network">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:text">@string/c_refresh</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="cornerRadius">54dp</item>
    </style>

    <style name="s_text_view_not_found">
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/fs_text_19</item>
        <item name="android:drawableTop">@drawable/ic_dm_not_search</item>
        <item name="android:drawablePadding">@dimen/i_p_16</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="s_text_view_error_network">
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/fs_text_19</item>
        <item name="android:drawableTop">@drawable/ic_dm_error_network</item>
        <item name="android:drawablePadding">@dimen/i_p_16</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="s_tool_bar_nav_icon">
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="tint">@color/white</item>
    </style>

    <style name="s_tool_bar_text">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="s_tool_bar_text_art">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="s_tool_bar_text_attr">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textFontWeight">100</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="s_search_text_view">
        <item name="android:background">@null</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:textColor">@color/black</item>
        <item name="drawableTint">@color/black</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:textColorHighlight">@color/c_caret_color</item>
        <item name="android:textCursorDrawable">@drawable/cursor_search</item>
    </style>

    <style name="s_text_view_song">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/fs_text_16</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="s_text_view_song_attr">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/fs_text_11</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/white</item>
        <item name="tint">@color/ico_gray</item>
    </style>

    <style name="s_iv">
        <item name="tint">@color/white</item>
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>

    <style name="s_iv_separate">
        <item name="tint">@color/white</item>
        <item name="android:layout_width">5dp</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">5dp</item>
    </style>

    <style name="s_iw_nw_not_error">
        <item name="android:src">@drawable/ic_dm_error_network</item>
    </style>

    <style name="s_search_bg">
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="s_clear_iw">
        <item name="tint">@color/black</item>
    </style>

    <style name="TabLayoutStyle">
        <item name="tabIndicatorColor">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">@dimen/fs_text_14</item>
    </style>

    <style name="colorMenu">
        <item name="android:background">@color/—Å_night_theme</item>
    </style>

    <style name="NavigationButtonStyle">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="itemTextColor">@drawable/bottom_navigate_color</item>
        <item name="android:textSize">@dimen/fs_text_12</item>
        <item name="itemIconTint">@drawable/bottom_navigate_color</item>
        <item name="backgroundTint">@color/black</item>
    </style>

    <style name="HeadStyle">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/fs_text_26</item>
        <item name="tint">@color/white</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="cornerRadius">@dimen/icp_9</item>
        <item name="iconGravity">textStart</item>
        <item name="android:drawablePadding">9dp</item>
        <item name="drawableTint">@color/black</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:shadowRadius">0</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="PlayListText">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
    </style>

    <style name="FirstContent">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">@dimen/icp_16</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="SecondContent">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">@dimen/fs_text_11</item>
        <item name="maxLines">1</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AudioPlayerImage">
        <item name="cornerRadius">@dimen/icp_8</item>
    </style>

    <style name="viewBottomEl">
        <item name="android:background">@color/white</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="inputTextStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/customize_et</item>
        <item name="hintTextColor">@color/blue</item>
        <item name="android:textColorHint">?attr/colorOnSecondary</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/fs_text_16</item>
    </style>

    <style name="create_button">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/fs_text_16</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:shadowRadius">0</item>
    </style>
</resources>